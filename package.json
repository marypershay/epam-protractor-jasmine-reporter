{
  "name": "epam-protractor-jasmine2-reporter",
  "version": "0.3.3",
  "description": "Use the screenshot reporter to capture screenshots after each executed Protractor test case.",
  "main": "json_report.js",
  "scripts": {
    "test": "mocha test",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "lint": "jshint --config .jshintrc index.js test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marypershay/epam-protractor-jasmine-reporter"
  },
  "dependencies": {
    "hat": "0.0.3",
    "lodash": "^3.0.0",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.4.3",
    "string.prototype.startswith": "^0.2.0",
    "uuid": "^2.0.0"
  },
  "keywords": [
    "screenshot",
    "jasmine",
    "jasmine2",
    "protractor",
    "selenium",
    "reporter",
    "html"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-fs": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-istanbul": "^1.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-mocha": "^2.2.0",
    "guppy-pre-commit": "^0.3.0",
    "istanbul": "^0.4.4",
    "jshint": "^2.9.2",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4"
  }
}
